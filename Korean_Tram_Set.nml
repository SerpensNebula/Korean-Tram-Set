
    grf {
        grfid                              :"KGT1"; 
        name                               :string(STR_GRF_NAME);
        desc                               :string(STR_GRF_DESC);
        url                                :string(STR_GRF_URL);
        version                            :230308;
        min_compatible_version             :230301;        
        
        // Parameters
            // speed & power parameters
                param  0    {
                    para_speed_power {
                        type:       int;
                        name:       string(STR_PARA_NAME_SPEED_POWER);
                        desc:       string(STR_PARA_DESC_SPEED_POWER);
                        min_value:  1;
                        max_value:  2;
                        def_value:  1;
                        
                        names:    {
                            1:    string(STR_PARA_OPT_1);
                            2:    string(STR_PARA_OPT_2);
                        };
                    }
                }
        }

// tables 
    cargotable {
        PASS
    }
    
    railtypetable {
        ELRL,
        KTR_,
        KTRC,
        KTRG
    }

// Sort
    sort(FEAT_ROADVEHS, [
        seoul_y1899,
        seoul_y1900,
        seoul_51,
        seoul_150,
        busan_old,
        seoul_363,
        seoul_381,
        Hyundai_wireless_tram_3cars,
        Hyundai_wireless_tram_5cars,
        Hyundai_wireless_tram_7cars,
        wirye,
    ]);    

// Graphic Templates of Tracks
    // GUI
        template tmpl_gui (x, y) {
            [ x,      y,      20,      13,       0,       0]  // first line
            [ x,      y,      20,      13,       0,       0]
            [ x+60,   y,      20,      20,       0,       0]
            [ x+90,   y,      20,      20,       0,       0]
            [ x+120,  y,      20,      20,       0,       0]
            [ x+150,  y,      20,      20,       0,       0]
            [ x,      y+30,   48,      36,       0,       0]  // second line, cursers
            [ x+60,   y+30,   48,      36,       0,       0]
            [ x+120,  y+30,   32,      32,       0,       0]
            [ x+160,  y+30,   32,      32,       0,       0]
            [ x+200,  y+30,   32,      32,       0,       0]
            [ x+240,  y+30,   32,      32,       0,       0]
        }

    // Tracks overlay
        template tmpl_overlay (x, y) {
            [ x,      y,      64,      31,     -31,       0] 
            [ x+70,   y,      64,      31,     -31,       0]
            [ x+140,  y,      64,      31,     -31,       0]  // crosses
            [ x+210,  y,      64,      31,     -31,       0]
            [ x+280,  y,      64,      31,     -31,       0]
            [ x+350,  y,      64,      31,     -31,       0]
            [ x+420,  y,      64,      31,     -31,       0]
            [ x+490,  y,      64,      31,     -31,       0]  // turns 
            [ x+560,  y,      64,      31,     -31,       0]
            [ x+630,  y,      64,      31,     -31,       0]
            [ x+700,  y,      64,      31,     -31,       0]
            [ x,      y+40,   64,      39,     -31,      -8]  // second line, slopes
            [ x+70,   y+40,   64,      23,     -31,       0]
            [ x+140,  y+40,   64,      23,     -31,       0]
            [ x+210,  y+40,   64,      39,     -31,      -8]
            [ x+280,  y+40,   64,      31,     -31,       0]  // ends
            [ x+350,  y+40,   64,      31,     -31,       0]
            [ x+420,  y+40,   64,      31,     -31,       0]
            [ x+490,  y+40,   64,      31,     -31,       0]
        } 

